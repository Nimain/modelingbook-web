pageTitle: 'One-box Model'
linkTitle: 'One-Box'
---------------------------------------------META
<script>
require(["libraries/jquery-1.7.2.js", "js/onebox.js"], function () {
    $(document).ready(function(){
		boxModel.init();
    });
});
</script>
   
<div class="content">
    <div class="page-header">
        <h1>One-box model</h1>
    </div>
    <div class="row">
        <div class="span16">
            <div class="examplespace workspace" id="modelboxcontainer">
                <div class="modelbox" id="modelbox1">
	                <p><span class="boxname"><em>M</em><sub>1</sub></span></p>
	                <p><span class="boxsize">Initial value: <span data-var="box1start"></span></span></p>
                    <!-- <p><span class="boxstart">Initial: <span data-var="box1start" class="TKAdjustableNumber inputField" data-min="0" data-max="200" data-step="1" data-format="%.0f"></span></span></p> -->
	                <span class="boxinput"><span data-var="box1in" class="TKAdjustableNumber inputField" data-min="0" data-max="200" data-step="1" data-format="%.0f"></span></span>
	                <span class="boxoutput"><span data-var="box1out" class="TKAdjustableNumber inputField" data-min="-1" data-max="1" data-step="0.01" data-format="%.2f"></span> <em>M</em><sub>1</sub></span>
                </div>
                <div class="modelbox" id="modelbox2">
	                <p><span class="boxname"><em>M</em><sub>2</sub></span></p>
	                <p><span class="boxsize">Initial value: <span data-var="box2start"></span></span></p>
                    <!-- <p><span class="boxstart">Initial: <span data-var="box2start" class="TKAdjustableNumber inputField" data-min="0" data-max="200" data-step="1" data-format="%.0f"></span></span></p> -->
	                <span class="boxinput"><span data-var="box2in" class="TKAdjustableNumber inputField" data-min="0" data-max="200" data-step="1" data-format="%.0f"></span></span>
	                <span class="boxoutput"><span data-var="box2out" class="TKAdjustableNumber inputField" data-min="-1" data-max="1" data-step="0.01" data-format="%.2f"></span> <em>M</em><sub>2</sub></span>
                </div>
                <div id="modelbox_equations">
                    \(\frac{\mathrm{d} M_1}{\mathrm{d} t} = \)<span data-var="box1in"></span> &ndash; <span data-var="a11" data-format="%.2f"></span> <em>M<sub>1</sub></em> + <span data-var="box2to1" data-format="%.2f"></span> <em>M<sub>2</sub></em><br>
                    \(\frac{\mathrm{d} M_2}{\mathrm{d} t} = \)<span data-var="box2in"></span> + <span data-var="box1to2" data-format="%.2f"></span> <em>M<sub>1</sub></em> &ndash; <span data-var="a21" data-format="%.2f"></span> <em>M<sub>2</sub></em>
                </div>
            </div>
            <div class="examplespace" id="graphcontainer">
                <div style ="width: 500px; float: left; margin-right: 20px">
					<div class="jsxbox" id="solution_board" style="height:300px;"></div>
				</div>
				<div style ="width: 300px; float: left;">
					<div class="jsxbox" id="phase_board" style="height:300px;"></div>
				</div>
				<div style="clear:both;"></div>
            </div>
        </div>
    </div>
</div>
<%= render_partial 'default_footer' %>